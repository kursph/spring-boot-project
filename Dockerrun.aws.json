{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "kursph-api",
      "image": "kurshjku/kursph-spring-boot-api:25.11.2023.10.48.48",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8080
        }
      ],
      "environment": [
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "value": "test"
        }
      ]
    }
  ]
}